{
  "name": "dobry-hasztag",
  "version": "1.0.0",
  "description": "WP Plugin by Everywhere",
  "main": "/src/ts/theme.ts",
  "scripts": {
    "dev": "npm-run-all --parallel webpack-watch tailwind-watch scss-watch",
    "build": "npm-run-all webpack tailwind scss",
    "webpack": "webpack",
    "webpack-watch": "webpack watch",
    "scss": "sass ./src/scss/theme.scss ./dist/css/theme.css",
    "scss-watch": "sass ./src/scss/theme.scss ./dist/css/theme.css -w",
    "tailwind": "tailwind -i ./src/scss/tailwind.css -o ./dist/css/tailwind.css",
    "tailwind-watch": "tailwind -i ./src/scss/tailwind.css -o ./dist/css/tailwind.css --watch"
  },
  "author": "Danylo Kupriienko (Everywhere Studio)",
  "license": "ISC",
  "devDependencies": {
    "dotenv-webpack": "^8.0.1",
    "tailwindcss": "^3.1.2",
    "ts-loader": "^9.2.8",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "dependencies": {
    "@types/select2": "^4.0.55",
    "archiver": "^5.3.1",
    "autoprefixer": "^10.4.7",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.3",
    "fetch": "^1.1.0",
    "form-data": "^4.0.0",
    "glightbox": "^3.2.0",
    "moment": "^2.29.4",
    "node-fetch": "^3.2.10",
    "npm-run-all": "^4.1.5",
    "poland-public-holidays": "^4.0.0",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^7.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.50.1",
    "simple-parallax-js": "^5.6.2",
    "simplebar": "^6.2.5",
    "style-loader": "^3.3.1",
    "swiper": "^8.2.4"
  }
}
